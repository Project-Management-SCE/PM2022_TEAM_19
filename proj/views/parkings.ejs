      <h4>type :<h5><%= post.type %> </h5>&nbsp;&nbsp;&nbsp;</h4>
      <h4>status :<h5><%= post.status %> </h5>&nbsp;&nbsp;&nbsp;</h4>
      <h4>price :<h5><%= post.price %> </h5>&nbsp;&nbsp;&nbsp;</h4>

      </p>
    </div>
    <% })%>
  </form>


</div>
